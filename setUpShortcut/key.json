[
    {
        "key": "cmd+.",
        "command": "editor.action.triggerSuggest",
        "when": "editorHasCompletionItemProvider && textInputFocus && !editorReadonly"
    },
    {
        "key": "ctrl+space",
        "command": "-editor.action.triggerSuggest",
        "when": "editorHasCompletionItemProvider && textInputFocus && !editorReadonly"
    },
    {
        "key": "cmd+enter",
        "command": "editor.action.formatDocument",
        "when": "editorHasDocumentFormattingProvider && editorHasDocumentFormattingProvider && editorTextFocus && !editorReadonly"
    },
    {
        "key": "shift+alt+f",
        "command": "-editor.action.formatDocument",
        "when": "editorHasDocumentFormattingProvider && editorHasDocumentFormattingProvider && editorTextFocus && !editorReadonly"
    },
    {
        "key": "ctrl+backspace",
        "command": "-deleteLeft",
        "when": "textInputFocus"
    },
    {
        "key": "cmd+l",
        "command": "editor.action.insertSnippet",
        "when": "editorTextFocus",
        "args": {
            "snippet": "console.log('ðŸš€${TM_SELECTED_TEXT}$1---->', ${TM_SELECTED_TEXT}$1);"
        }
    },
    {
        "key": "ctrl+/",
        "command": "turboConsoleLog.displayLogMessage"
    },
    {
        "key": "ctrl+alt+l",
        "command": "-turboConsoleLog.displayLogMessage"
    },
    {
        "key": "shift+cmd",
        "command": "turboConsoleLog.uncommentAllLogMessages"
    },
    {
        "key": "shift+alt+u",
        "command": "-turboConsoleLog.uncommentAllLogMessages"
    },
    {
        "key": "ctrl+alt+l",
        "command": "workbench.debug.action.toggleRepl",
        "when": "workbench.panel.repl.view.active"
    },
    {
        "key": "shift+cmd+y",
        "command": "-workbench.debug.action.toggleRepl",
        "when": "workbench.panel.repl.view.active"
    },
    {
        "key": "ctrl+cmd+k",
        "command": "turboConsoleLog.commentAllLogMessages"
    },
    {
        "key": "shift+alt+c",
        "command": "-turboConsoleLog.commentAllLogMessages"
    },
    {
        "key": "cmd+u",
        "command": "editor.action.insertSnippet",
        "when": "editorTextFocus",
        "args": {
            "snippet": "const [${1},  set${1/(.*)/${1:/capitalize}/}] = useState($2);"
        }
    },
    {
        "key": "alt+s",
        "command": "editor.action.insertSnippet",
        "when": "editorTextFocus",
        "args": {
            "snippet": "setTimeout(()=> {$1},$2)"
        }
    },
    {
        "key": "cmd+e",
        "command": "editor.action.insertSnippet",
        "when": "editorTextFocus",
        "args": {
            "snippet": "const $1 = ($2) => { $3 }"
        }
    },
    {
        "key": "cmd+escape",
        "command": "editor.action.insertSnippet",
        "when": "editorTextFocus",
        "args": {
            "snippet": "if (typeof window === 'undefined') return;"
        }
    },
    {
        "key": "cmd+3",
        "command": "editor.action.insertSnippet",
        "when": "editorTextFocus",
        "args": {
            "snippet": "onClick={() => $1($2)}"
        }
    },
    {
        "key": "cmd+2",
        "command": "editor.action.insertSnippet",
        "when": "editorTextFocus",
        "args": {
            "snippet": "onClick={$1}"
        }
    },
    {
        "key": "cmd+i",
        "command": "editor.action.insertSnippet",
        "when": "editorTextFocus",
        "args": {
            "snippet": "if($1){ $2 }"
        }
    },
    {
        "key": "cmd+1",
        "command": "editor.action.insertSnippet",
        "when": "editorTextFocus",
        "args": {
            "snippet": "useEffect(()=> {$1},[$2])"
        }
    },
    {
        "key": "cmd+m",
        "command": "editor.action.insertSnippet",
        "when": "editorTextFocus",
        "args": {
            "snippet": "$1.map((item,index)=><$2 key={index}></$2>)"
        }
    },
    {
        "key": "alt+r",
        "command": "editor.action.insertSnippet",
        "when": "editorTextFocus",
        "args": {
            "snippet": "return"
        }
    },
    {
        "key": "cmd+p",
        "command": "workbench.action.quickOpen"
    }
]